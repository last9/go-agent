# Last9 OTLP Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=https://otlp.last9.io
OTEL_EXPORTER_OTLP_HEADERS=Authorization=Basic YOUR_AUTH_TOKEN_HERE

# Service Configuration
OTEL_SERVICE_NAME=gin-full-example
OTEL_RESOURCE_ATTRIBUTES=deployment.environment=development,team=platform,version=1.0.0

# Trace Sampling
OTEL_TRACES_SAMPLER=always_on

# Database Configuration (optional)
DATABASE_URL=postgres://postgres:postgres@localhost/example?sslmode=disable

# Redis Configuration (optional)
REDIS_URL=localhost:6379
